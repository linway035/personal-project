<script>
  function logout() {
    fetch('/users/logout', {
      method: 'GET',
    })
      .then(response => {
        localStorage.removeItem('userId')
        localStorage.removeItem('name')
        window.location.href = '/users/signin'
      })
      .catch(error => {
        console.error(error)
      })
  }
</script>
